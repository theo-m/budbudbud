{
  "name": "budbudbud",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "concurrently --kill-others-on-fail -n NEXT,TSCW,PRSM -c magenta,cyan,red \"next dev\" \"yarn tscw\" \"prisma generate --watch\"",
    "tscw": "tsc --noEmit --incremental --preserveWatchOutput --pretty --watch",
    "build": "next build",
    "prebuild": "prisma generate",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "3.12.0",
    "@trpc/client": "^9.22.0",
    "@trpc/next": "^9.22.0",
    "@trpc/react": "^9.22.0",
    "@trpc/server": "^9.22.0",
    "classnames": "^2.3.1",
    "handlebars": "^4.7.7",
    "html-to-text": "^8.2.0",
    "inline-css": "^3.0.0",
    "mjml": "^4.12.0",
    "next": "12.1.4",
    "next-auth": "^4.3.1",
    "nodemailer": "^6.7.3",
    "postmark": "^3.0.1",
    "prisma": "^3.12.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.29.0",
    "react-query": "^3.34.19",
    "superjson": "^1.8.1",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@types/html-to-text": "^8.1.0",
    "@types/inline-css": "^3.0.1",
    "@types/mjml": "^4.7.0",
    "@types/node": "17.0.23",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "slugify": "^1.6.5",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "4.5.4"
  }
}
